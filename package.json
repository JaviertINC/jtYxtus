{
    "name": "@javiertinc/yxtus",
    "version": "1.0.1",
    "description": "Un procesador de texto plano a HTML.",
    "author": "@JaviertINC",
    "homepage": "https://github.com/JaviertINC/jtYxtus/wiki",
    "repository": "github:javiertinc/jtYxtus",
    "license": "MIT",
    "keywords": [
        "javiertinc",
        "yxtus",
        "typescript",
        "javascript",
        "library",
        "utils",
        "markdown",
        "latex"
    ],
    "type": "module",
    "main": "./index.js",
    "exports": {
        ".": {
            "import": "./index.js",
            "require": "./index.js"
        }
    },
    "scripts": {
        "clear": "rm -r ./dist",
        "dev": "jtexec ./src/index.ts",
        "build": "tsc && node ./pkgBuild.cjs",
        "test": "ts-node test/test.ts",
        "test:output": "ts-node test/test.ts > test/test-results.txt",
        "publish": "npm run build && cd dist && npm publish --registry http://localhost:4873",
        "min": "browserify ./dist/index.js -o ./dist/jt.yxtus.bundle.js && uglifyjs ./dist/jt.yxtus.bundle.js -o ./dist/jt.yxtus.min.js"
    },
    "devDependencies": {
        "browserify": "^17.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "uglify-js": "^3.19.3"
    }
}
